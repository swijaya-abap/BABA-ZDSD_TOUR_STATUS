	xmlns:layout="sap.ui.layout">
	<App id="idAppControl">
		<Page showHeader="true" title="Tour Status" showFooter="true">
			<content>
				<Label text="Branch" design="Bold" width="200px" required="true" textAlign="Begin" textDirection="Inherit" visible="true"
