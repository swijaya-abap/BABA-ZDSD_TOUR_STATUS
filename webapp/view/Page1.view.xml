					<core:Item text="{Branch} - {BranchName}" enabled="true" key="{Branch}"/>
				</MultiComboBox>
				<Label text="Visit Plan Type" design="Bold" width="200px" required="true" textAlign="Begin" textDirection="Inherit" visible="true"
					class="sapUiResponsiveMargin"/>
