								</Table>
							</content>
						</IconTabFilter>
					</items>
					<content/>
				</IconTabBar>
			</content>
			<footer>
				<OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
					<content>
						<ToolbarSpacer width=""/>
						<Button text="Get Data" type="Default" icon="sap-icon://repost" iconFirst="true" width="auto" enabled="true" visible="true"
							iconDensityAware="false" press="onButtonPress"/>
						<Button text="Resend Tour" type="Default" icon="sap-icon://synchronize" iconFirst="true" width="auto" enabled="true" visible="{modelView>/resendVisible}"
							iconDensityAware="false" press="onResendPress"/>
						<Button text="Remove Tour" type="Default" icon="sap-icon://delete" iconFirst="true" width="auto" enabled="true" visible="{modelView>/removeVisible}"
